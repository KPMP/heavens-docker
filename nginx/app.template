server {
    listen ${ENV_APP_PORT};
    server_name ${ENV_APP_HOSTNAME};
    index index.html index.htm;
    location /api {
        proxy_pass http://docker-spring/;

    }
    location /api/v1/metadatas {
        proxy_pass http://docker-metadata/v1/metadatas;
    }
    location / {
        root /usr/share/nginx/html/build;
        try_files $uri /index.html;
    }
}

